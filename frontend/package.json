{
  "name": "enterprise-pos-frontend",
  "version": "1.0.0",
  "description": "Enterprise POS System Frontend - React PWA",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:pages": "vite build && echo 'enterprise-pos.pages.dev' > dist/CNAME",
    "deploy": "npm run build:pages && npx wrangler pages publish dist",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "npx vite-bundle-analyzer",
    "pwa:generate": "pwa-asset-generator public/logo.svg public/icons --background '#1890ff' --padding '10%'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "antd": "^5.12.8",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/colors": "^7.0.2",
    "@tanstack/react-query": "^5.14.2",
    "@tanstack/react-query-devtools": "^5.14.2",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "recharts": "^2.8.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-to-print": "^2.14.15",
    "react-error-boundary": "^4.0.11",
    "react-helmet-async": "^1.3.0",
    "framer-motion": "^10.16.16",
    "lodash": "^4.17.21",
    "zustand": "^4.4.7",
    "immer": "^10.0.3",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-windicss": "^1.9.3",
    "windicss": "^3.5.6",
    "typescript": "^5.3.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "vitest": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "jsdom": "^23.0.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "vite-bundle-analyzer": "^0.7.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-tsconfig-paths": "^4.2.2",
    "pwa-asset-generator": "^6.3.1",
    "wrangler": "^3.22.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "pos",
    "react",
    "pwa",
    "antd",
    "enterprise",
    "cloudflare",
    "vite",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/namhbcf1/cloudflare-enterprise-pos"
  },
  "homepage": "https://enterprise-pos.pages.dev",
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:react/jsx-runtime"
    ],
    "ignorePatterns": [
      "dist",
      ".eslintrc.cjs"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react-refresh"
    ],
    "rules": {
      "react-refresh/only-export-components": [
        "warn",
        {
          "allowConstantExport": true
        }
      ],
      "@typescript-eslint/no-unused-vars": "warn",
      "react/prop-types": "off",
      "react/display-name": "off"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 100,
    "jsxSingleQuote": true,
    "endOfLine": "lf"
  },
  "postcss": {
    "plugins": {
      "windicss/plugin": {}
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}